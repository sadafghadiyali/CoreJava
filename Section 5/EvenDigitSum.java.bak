public class EvenDigitSum {
    // write your code here
    
    public static int getEvenDigitSum(int num){
        if(num>=0){
        int n=num;
        int sum=0;
        while(n>0){
            if(n%2==0){
                sum=sum + n%10;
                
            }
            n=n/10;
        }
        return sum;
    }
    return -1;
    }
    
    
}