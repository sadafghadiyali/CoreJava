public class NumberPalindrome {
    
    public static boolean isPalindrome(int num){
        
        //int sum=0;
        int rev=0;
        int n=Math.abs(num);
        
        while(n>0){
            rev=rev*10 + n%10;
            n=n/10;
        }
        
        //System.out.println(rev);
        
        if(rev==Math.abs(num))
            return true;
        else
            return false;
    }
 
}