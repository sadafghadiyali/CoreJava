public class GreatestCommonDivisor {
    // write your code here
    public static int getGreatestCommonDivisor(int n1,int n2){
        if(n1<10 || n2<10){
            return -1;
        }else{
            int GCD=0;
            int tempL,tempS;
            
            if(n1>n2){
                tempL=n1;
                tempS=n2;
            }
            else{
                tempL=n2;
                tempS=n1;
            }
            if(tempL%tempS == 0)
                return tempS;
            else{
            for (int i=tempS/2;i>=2;i--){
                //System.out.println(i);
                if(tempS%i == 0 && tempL%i ==0){
                    GCD=i;
                    
                    break;
                } 
            }
            
            return GCD;
            }
        }
    }
    
}